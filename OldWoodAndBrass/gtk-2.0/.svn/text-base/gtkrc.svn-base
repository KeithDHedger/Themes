#odd ones

style "theme-tooltips"
{
	fg[NORMAL]="#000000"
	bg[NORMAL]="#c0c0c0"
}

style	"combo"
{
  	fg[NORMAL]="#000000"
	fg[ACTIVE]="#404040"
	fg[PRELIGHT]="#808080"
}

style "treeview"
{
  	fg[NORMAL]="#000000"
	fg[ACTIVE]="#000000"
	fg[PRELIGHT]="#000000"
	fg[SELECTED]="#000000"
}

style "notebook"
{
  	fg[NORMAL]="#ffffff"
	fg[ACTIVE]="#000000"
